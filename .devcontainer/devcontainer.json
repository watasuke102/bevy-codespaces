{
  "name": "bevy-codespaces",
  "service": "bevy",
  "remoteUser": "vscode",
  "workspaceFolder": "/bevy",
  "dockerComposeFile": "docker-compose.yml",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "ritwickdey.LiveServer",
        "dustypomerleau.rust-syntax",
        "eamodio.gitlens",
        "bungcip.better-toml"
      ]
    }
  }
}
